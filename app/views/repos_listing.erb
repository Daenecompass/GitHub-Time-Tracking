<h1>Repository Listing</h1>
<table class="table">
	<tbody>
		<tr>
			<th>Username</th>
			<th>Repository Name</th>
			<th>Full Repository Name</th>
			<th></th>
			<th></th>
		</tr>
		<% @reposList.each do |r| %>
			<tr>
				<td><%= r["username"] %></td>
				<td><%= r["repo_name"] %></td>
				<td><%= r["repo_name_full"] %></td>
				<td><a href="/<%= r["repo_name_full"] %>/issues">Issues</a></td>
				<td><a href="/<%= r["repo_name_full"] %>/milestones">Milestones</a></td>
			</tr>
		<% end %>
	</tbody>
</table>
