<table class="table">
	<tbody>
		<tr>
		  <th>Repository</th>
		  <th>Milestone #</th> 
		  <th>Issue #</th>
		  <th>Issue Title</th>
		  <th>Issue State</th>
		  <th>Time Duration Sum (Time Comment Count)</th>
		</tr>
<% @issuesInMilestone.each do |i| %>
	<tr>
		<td><%= i["repo_name"] %></td>
		<td><%= i["milestone_number"] %></td>
		<td><%= i["issue_number"] %></td>
		<td><%= i["issue_title"] %></td>
		<td><%= i["issue_state"] %></td>
		<td><%= i["time_duration_sum"] %>(<%= i["time_comment_count"] %>)</td>
	</tr>
<% end %>
  </tbody>
</table>  
